#walllist_answer_ajax
  -@posts.each do |p|
    .post_body
      %ul
        %p.post_photo= image_tag(p.photo, :size => "50x50")      
        %p.post_title= link_to p.title, wall_path(p)
        %p.post_content= p.content
        .icons
          = link_to( image_tag("edit.png", :size => "22x22"), edit_wall_path(p))
          = link_to( image_tag("del.png", :size => "22x22"),  "/wall/#{p.id}", :confirm => 'Are you sure?', :method => :delete)